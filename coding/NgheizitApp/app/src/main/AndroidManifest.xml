<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pc_08.myapplication">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <!-- 启动拨打电话权限 -->
    <uses-permission android:name="android.permission.CALL_PHONE"/>


    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/dwarfs"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/dwarfs"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DirActivity"></activity>
        <activity android:name=".CallphoneActivity"></activity>
        <activity android:name=".GetInternetActivity"></activity>
    </application>

</manifest>